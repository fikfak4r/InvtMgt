			"\t<div class=\"form-group\" >",
			"\t<label for=\"${1:columnName}\">${2:Label}</label>",
			"\t<input type=\"${3|email,number,password|}\" class=\"form-control\" id=\"${1:columnName}\" ${4:data-role=\"${5|datepicker,datetimepicker,grid,autocomplete|}\"} ${6:data-bind=\"${7:name-value}\"} ${8:placeholder=\"${9:placeHolder}\"}>",
		"\t</div>"
		 ]
	}



